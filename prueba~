class Test:
    x = 10
    def main():
        recursivo(1)
        y = 2
        x = 5 + y
        z = suma(y, x)
        print(z)
        count = 1
        while count < 10:
            print(count)
            count = count + 1
        if z == 9 :
            print(200)
        else
            print(100)
        for z : 12
            print(1)

    def suma(num1: int, num2: int):
        result = num1 + num2
        return result

    def recursivo(num:int):
        if num == 10 :
            return 1
        else
            num2 = num + 1
            return recursivo(num2)

